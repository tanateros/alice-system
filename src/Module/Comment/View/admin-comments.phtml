{% for comment in comments %}
<div id="callout-third-party-libs" class="bs-callout bs-callout-warning">
    <a href="/comment/edit&id={{ comment.id }}" class="btn btn-primary">Редактировать</a>
    <address><a href="mailto:{{ comment.getEmail() }}">{{ comment.getName() }}</a></address>
    <p>{{ comment.getText() }}</p>
</div>
{% endfor %}
